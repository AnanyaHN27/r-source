data <- function(..., list=character(0))
{
	names <- c(as.character(substitute(list(...))[-1]), list)
	if(length(names) == 0) {
		system(paste("$RHOME/cmd/pager", system.file("data",
			"index.doc")))
	}
	else
		for(name in names) {
			file <- system.file("data", name)
			if(file == "") stop(paste("no data set called", name))
			else source(file)
	}
	invisible(names)
}
