TITLE(Devices @@ List of Graphical Devices)
USAGE(
postscript(DOTS)
x11(DOTS)
windows(DOTS)
macintosh(DOTS)
)
ALIAS(Devices)
ALIAS(device)
ALIAS(windows)
COMMENT(^^^^^ 'windows' does not yet have its own doc page !!
  -----------------
 |||||  'VALUES' below should be replaced by 'DESCRIPTION' +  'ITEMIZE'!
 vvvvv)
VALUES(The following graphics devices are currently available:
@@
ITEM(LANG(LINK(postscript)) @@ writes graphics into PostScript file)
ITEM(LANG(LINK(x11)) @@  The graphics driver for the  X11 Window
    system (unix))
ITEM(LANG(LINK(pictex)) @@  writes LaTeX/PicTeX graphics commands into file)
ITEM(LANG(LINK(macintosh)) @@ The graphics driver for the Macintosh)
ITEM(LANG(LINK(windows)) @@ The graphics driver for MS Windows)
)
SEEALSO(
the individual help files for further information on any of the
devices listed here.
PARA
LANG(LINK(print.plot)), LANG(LINK(save.plot))
LANG(LINK(graphics.off)), LANG(LINK(image)).
)
KEYWORD(device)
