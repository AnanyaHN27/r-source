TITLE(complete.cases @@ Find Complete Cases)
USAGE(
complete.cases(DOTS)
)
ALIAS(complete.cases)
ARGUMENTS(
ARG(DOTS @@ a sequence of vectors, matrices and data frames.)
)
VALUE(
A vector specifying which
observations/rows have no missing values across the entire sequence.
)
SEEALSO(
LANG(LINK(na.omit)), LANG(LINK(na.fail)).
)
EXAMPLES(
# x is a regression design matrix
# y is the corresponding response
ok <- complete.cases(x,y)
x <- x[ok,]
y <- y[ok]
)
